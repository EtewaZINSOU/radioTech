                    </div>
                    <div class="col-sm-6">
                        {% image '@PylosStoreBundle/Resources/public/images/home/girl3.jpg'%}
                        <img src="{{ asset_url }}" alt="Example" class="girl img-responsive"/>
                        {% endimage %}

                        {% image '@PylosStoreBundle/Resources/public/images/home/pricing.png' %}
