                    <div class="col-sm-6">
                        {% image '@PylosStoreBundle/Resources/public/images/home/girl1.jpg'%}
                            <img src="{{ asset_url }}" alt="Example" class="girl img-responsive"/>
                        {% endimage %}

                        {% image '@PylosStoreBundle/Resources/public/images/home/pricing.png' %}
                        <img src="{{ asset_url }}" alt="Example" class="pricing"/>

                        {% endimage %}
                    </div>

                </div>
            </div>
            <!-- Page 2 -->
