{% extends "RTechAppBundle::layout.html.twig" %}
{% block body %}
{% block rtech_user_content %}
    <div class="fos_user_user_show" xmlns="http://www.w3.org/1999/html">
        <div class="panel panel-info profile_info">
            <div class="panel-heading">Liste des categories</div>
            <div class="panel-body">
                <ul class="row">
                    <div class="col-md-12">
                        {% for categorie in categories %}
                           <p><strong>Nom de la categorie:</strong>  {{ categorie.categoryName }}</p><br>
                        <p><strong>Description:</strong>   {{ categorie.description }}</p><br><hr>
                        {% endfor %}
                    </div>
                </ul>
            </div>
        </div>
    </div>
{% endblock rtech_user_content %}
{% endblock %}